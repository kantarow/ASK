<% @users.each do |user| %>
  <%= link_to user.id_name, user %>
  <br>
<% end %>

<% @items.each do |item| %>
  <%= link_to item, class: "item-headline-link" do %>
    <div class="item-headline">
      <h5><%= item.title %></h5>
      <p><%= "by " + item.user.id_name %></p>
      <hr>
    </div>
  <% end %>
<% end %>
